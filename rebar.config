{erl_opts, [
    debug_info
]}.

{minimum_otp_vsn, "19.3"}.

{deps, []}.

{ct_readable, false}.

{edoc_opts, [
    {dir, "docs"}
]}.

{plugins, [rebar3_hex]}.

{xref_checks, [
    undefined_function_calls,
    undefined_functions,
    locals_not_used,
    deprecated_function_calls,
    deprecated_functions
]}.
